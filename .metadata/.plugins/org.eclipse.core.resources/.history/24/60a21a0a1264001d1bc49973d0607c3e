package SearchingAlgo;

import java.util.Scanner;

public class square_root_using_binarySearch {

	public static void main(String[] str)
	{
		
		Scanner sc= new Scanner(System.in);
		int num= sc.nextInt();
		int start=0;
		int end= num;
		int mid= (start+end)/2;
		long result=-1;
		
		// Find only the integer part
		
		while(start<=end)
		{
			int square= (mid*mid);
			
			if(square==num)
			{
				return;
			}
			
		}
		
	}
}
