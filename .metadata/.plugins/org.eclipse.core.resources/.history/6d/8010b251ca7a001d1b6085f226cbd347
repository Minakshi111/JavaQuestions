package Recursion;

public class subSetOfString {
	public static void main(String[] str)
	{
		
		String s= "123";
		
		check_subsets(s, 0, "");
		
	}

	private static void check_subsets(String s, int count, String out) {
		
		if(count==s.length())
		{			
			System.out.println(out);
			return;
		}
		
		// include call
		char ele= s.charAt(count);
		out=out+ele;
		check_subsets(s, count+1, out);
		
		
		// excluude call
		check_subsets(s, count+1, out);
		
	}

}
