package SearchingAlgo;

import java.util.Scanner;

public class square_root_using_binarySearch {

	public static void main(String[] str)
	{
		
		Scanner sc= new Scanner(System.in);
		int num= sc.nextInt();
		int start=0;
		int end= num;
		int mid= (start+end)/2;
		int result=0;
		// Find only the integer part
		
		while(start<=end)
		{
			long  square= (mid*mid);
			
			if(square==num)
			{
				result=mid;
				System.out.println("Square root is: "+result);
				return;
			}
			else if(square>num)
			{
				end= mid-1;
			}
			else if(square<num)
			{
				if(result<mid)
				result=mid;
				start=mid+1;
				
			}
			mid=(start+end)/2;
			
		}
		
		System.out.println("Square root is: "+result);
		
	}
}
