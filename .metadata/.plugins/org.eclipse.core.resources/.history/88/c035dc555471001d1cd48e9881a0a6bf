package Recursion;

public class binarySearch {
	static int target;
	public static void main(String[] str)
	{
		int arr[]= {2,4,6,8,9};
		
		int index= binarySearch(arr, 0, arr.length-1 );
		 target= 8;
		
	}

	private static int binarySearch(int[] arr, int s, int e) {

		int mid= (s+e)/2;
		
		if(arr[mid]==target)
		{
			return mid;
		}
		
		if(s>=e)
		{
			return -1;
		}
		
		
		

	}
	

}
