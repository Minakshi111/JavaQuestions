package javaPackage;

public class UnaryOperator {
	
	public static void main(String[] args) {
		
		int a= 10;  
		int b=10;
		System.out.println(a++);  //10
		System.out.println(a++ + ++a); //11+13
		System.out.println(b++ + ++b); // 10 11 12 12 
		
		int x= 2;
		int y=x++;
		
		System.out.println(y);  // 2 
		System.out.println(x); //3
		
	}

}
