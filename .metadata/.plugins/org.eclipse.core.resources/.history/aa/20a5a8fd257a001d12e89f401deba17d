package Recursion;

public class subSetOfString {
	public static void main(String[] str)
	{
		
		int arr[]= {1,2,3};
		int out[]= new int[arr.length];
		
		check_subsets(arr, 0, out,0);
		
	}

	private static void check_subsets(int[] arr, int count, int out[], int j) {
		
		if(count==arr.length)
		{
			for ( int i=0; i<out.length;i++)
				System.out.println(out[i]);
			return;
		}
		
		// include call
		int ele= arr[count];
		out[j]= ele;
		check_subsets(arr, count+1, out, j+1);
		
		
		// excluude call
		check_subsets(arr, count+1, out, j);
		
	}

}
