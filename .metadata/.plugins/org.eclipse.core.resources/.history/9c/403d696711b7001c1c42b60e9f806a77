public class SumOf2Num {
	
	
	public static int[] findNums(int[] num, int target)
	{
		HashMap<Integer, Integer> hmap= new HashMap<Integer, Integer>();
		
		for ( int i=0; i<num.length; i++)
		{
			hmap.put(i, num[i]);		
		}
		
		
	
	}
	
	public static void main(String[] args) {
		
		
			
	}

}
