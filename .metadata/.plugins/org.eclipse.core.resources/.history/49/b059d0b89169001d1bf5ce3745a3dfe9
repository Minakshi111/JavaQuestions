package Sorting;

import java.util.Scanner;

public class InsertionSort {

	public static void main(String [] str)
	{
		Scanner sc= new Scanner(System.in);
		int n= sc.nextInt();		
		int arr[]= new int[n];
		
		for( int i=0; i<n;i++)
		{
			arr[i]= sc.nextInt();
		}
		
		
		// Implement Insertion sort here
		
		for ( int i=1; i<n;i++)
		{		
			int temp= arr[i];
			int j;
			
			for (  j=(i-1); j>=0; j--)
			{
				int temp1= arr[j];
				
				if(temp < arr[j])
				{
					arr[j+1]= arr[j];
					System.out.println("shift : arr[j+1] "+arr[j+1]);
				}
				else
				{
					arr[j]= temp;
				}
			}
			
			
		}
		
		// Print the sorted array
		
		for( int i=0; i<n;i++)
		{
			System.out.println(arr[i]);
		}
	}
	
}
