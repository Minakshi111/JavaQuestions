	{
		
		Scanner sc= new Scanner(System.in);
		int n= sc.nextInt();
		int m= sc.nextInt();
		int arr[]= new int[n];
		
		for ( int i=0; i<n;i++)
		{
			arr[i]= sc.nextInt();
		}
		
	}
}
