package javaPackage;

public class Variable4 {	
	public static void main(String[] args) {
		int a[]= {1,2,3,4,5,6,100};  // integer araay , total value = 7, size of array= 7
		
		String b[]= {"Ram", "SHyam", "Mina"}; //String array
		
		System.out.println(b[0]);
		System.out.println(a[4]);
		
		// Store all data types in one array
		
		// This is called Object array:
		Object c[]= {1,2,3, "Ram", 3>12};
		
		System.out.println(c[3]);
	}
}
