package Recursion;

public class ReverseString {
	public static void main(String[] str)
	{
		
		String s= "ABCDE";
		char arr[]= s.toCharArray();
		
		String s1= reverse(arr, 0, s.length()-1);
	}

	private static String reverse(char[] arr, int i, int j) {
		
		// Base case
		
		if(i>j)
		{
			 
		}
		
		// swap 2 string
		char temp= arr[i];
		arr[i]= arr[j];
		arr[j]= temp;
		
		
		
		
		
		return null;
	}

}
