package SearchingAlgo;

import java.util.Scanner;

public class total_occurences {
	static int result=0;
	public static void main(String [] str)
	{
		Scanner sc= new Scanner(System.in);
		int n= sc.nextInt();
		int arr[]= new int[n];
		for (int i=0; i<n; i++)
		{
			arr[i]= sc.nextInt();
		}
		
		int num= sc.nextInt();
       int mid= (0+arr.length)/2;
		// find the num's first and last occurence in this array
		// input : 
		/* 8
		 * 0 0 1 1 2 2 2 2
		 * 2
		 * output= 4 7
		 */
		
		
		// Implement the binary search algo here
       lastOccurence(arr, n, num,0,arr.length-1, mid);	
      
       
       System.out.println("Result is : "+result);
		 
		
	}
	
private static void lastOccurence(int[] arr, int n, int num, int start, int end, int mid) {
		
		while(start<=end)
		{
		//	int mid= start+(end-start)/2;
			 mid=(start+end)/2;
			
			if(arr[mid]==num)
			{				
				result= result+1;
				start=mid+1;	// right
			//	end=mid-1;    // left
			}
			else if(arr[mid]>num)
			{
				end= mid-1;
			}
			else if(arr[mid]<num)
			{
				start=mid+1;
			}
			
		}
		
	}

}
